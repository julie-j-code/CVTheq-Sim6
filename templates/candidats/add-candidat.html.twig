{% extends 'template.html.twig' %}

{% block title %}Création d'un candidat{% endblock %}

{% block PageTitle %}
Formulaire d'ajout d'un candidat à la CvTheque
{% endblock %}

{% block body %}
<div class="row">

{{form_start(form)}}
<div class="row">
    <div class="col">{{ form_row(form.firstname) }}</div>
    <div class="col">{{ form_row(form.lastname) }}</div>
</div>

{{ form_end(form)}}


</div>


{% endblock %}